{
  "name": "bulak-smart-connect-js",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:start-backend\" \"npm:start-frontend\"",
    "start-backend": "cd ../bsc-js-backend && npm run start:dev",
    "start-frontend": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:fix-react": "npm run lint:fix && npm run fix-react-imports",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\"",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:a11y": "jest --config=jest.a11y.config.js --testMatch='**/*.a11y.test.{js,jsx}'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@mui/x-charts": "^8.5.3",
    "@mui/x-date-pickers": "^7.29.4",
    "@react-pdf/renderer": "^4.3.0",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-router": "^7.6.2",
    "react-router-dom": "^7.6.2",
    "react-scan": "^0.3.4",
    "recharts": "^2.15.3",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.14.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.0.7"
  }
}
